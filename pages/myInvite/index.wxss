/* pages/myInvite/index.wxss */
page{
  background: #fff;
}

.myInvite{
  display: flex;
  align-items: center;
  padding: 40rpx 32rpx;
  border-bottom: 2rpx solid #f0f0f0;
}
.avatar{
  width: 108rpx;
  height: 108rpx;
  margin-right: 18rpx;
  border-radius: 54rpx;
}
.userInfo{
  display: flex;
  flex-direction: column;

}
.name{
  font-size: 32rpx;
  color: #333;
  padding-bottom: 8rpx;
}
.date{
  font-size: 24rpx;
  color: rgba(34,34,34,.38);
}