/* ./commpents/navbar/index.wxss */
.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  color: #000;
  z-index: 9999999;
  overflow: hidden;
}

.logo{
  width: 94rpx;
  height: 68rpx;
  position: absolute;
  top: 50%;
  /* margin-top: -34rpx; */
  left: 32rpx;
}

/* 背景图 */
.backgroundimg {
  display: flex;
  position: absolute;
  z-index: 0;
  width: 100%;
}

/* 标题要居中 */
.nav-title {
  position: absolute;
  text-align: center;
  max-width: 400px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 20px;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.nav-capsule {
  display: flex;
  align-items: center;
  margin-left: 15px;
  width: 140px;
  justify-content: space-between;
  height: 100%;
}

.back-pre {
  width: 94rpx;
  height: 68rpx;
  margin-top: 4px;
  position: relative;
  z-index: 1;
}

.nav-capsule {
  width: 94rpx;
  height: 68rpx;
  margin-top: 3px;
}